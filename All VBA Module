Sub SORT()
'
' SORT Macro
'
' Keyboard Shortcut: Ctrl+Shift+F
'
    Selection.Copy
    Sheets("PATIENT DATA").Select
    Rows("6:6").Select
    Range("D6").Activate
    Application.CutCopyMode = False
    Range("F6").Select
    ActiveSheet.Range("$F$6:$J$23").AutoFilter field:=1
    ActiveSheet.Range("$F$6:$J$23").AutoFilter field:=2
    Range("H6").Select
    ActiveSheet.Range("$F$6:$J$23").AutoFilter field:=3
    ActiveSheet.Range("$F$6:$J$23").AutoFilter field:=5
    Sheets("PRINT").Select
    Selection.Copy
    Sheets("PATIENT DATA").Select
    ActiveSheet.Range("$F$6:$J$23").AutoFilter field:=2, Criteria1:="230"
    Sheets("PATIENT DATA").Select
    Range("H12:H23").Select
    Sheets("PRINT").Select
    ActiveCell.Offset(1, -3).Range("A1").Select
    Sheets("PATIENT DATA").Select
    Range("J12:J23").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("PRINT").Select
    Range("E8").Select
    ActiveSheet.Paste
    Application.CutCopyMode = False
    With Selection.Validation
        .Delete
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, Operator:= _
        xlBetween, Formula1:="=$E$8:$E$19"
        .IgnoreBlank = False
        .InCellDropdown = True
        .InputTitle = ""
        .ErrorTitle = "Select correct format"
        .InputMessage = ""
        .ErrorMessage = ""
        .ShowInput = True
        .ShowError = True
    End With
    Range("E8").Select
    Selection.Copy
    Sheets("PATIENT DATA").Select
    ActiveSheet.Range("$F$6:$J$23").AutoFilter field:=5, Criteria1:="LFT"
    ActiveWindow.ScrollColumn = 5
    ActiveWindow.ScrollColumn = 7
    ActiveWindow.ScrollColumn = 8
    ActiveWindow.ScrollColumn = 9
    ActiveWindow.ScrollColumn = 10
    ActiveWindow.ScrollColumn = 11
    ActiveWindow.ScrollColumn = 13
    ActiveWindow.ScrollColumn = 14
    ActiveWindow.ScrollColumn = 15
    ActiveWindow.ScrollColumn = 16
    ActiveWindow.ScrollColumn = 17
    ActiveWindow.ScrollColumn = 19
    ActiveWindow.ScrollColumn = 20
    ActiveWindow.ScrollColumn = 21
    ActiveWindow.ScrollColumn = 22
    ActiveWindow.ScrollColumn = 23
    ActiveWindow.ScrollColumn = 25
    ActiveWindow.ScrollColumn = 26
    ActiveWindow.ScrollColumn = 27
    Range("AG15:AO15").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("LFT").Select
    Range("E21").Select
    Selection.PasteSpecial Paste:=xlPasteAll, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=True
    Range("F21").Select
End Sub
