Sub HYPERLINK()

Dim path As String
path = "file:///C:\Users\ManiKandan\Desktop\New folder\clinic\reports\"
ActiveSheet.Select
Dim regno As String
regno = Range("i14")

Dim filename1 As String
filename1 = regno & "-" & Range("b12").Text & "-" & Range("A19")
Filename = path & filename1
'MsgBox (Filename)
With Worksheets(6)
Sheets("PATIENT DATA").Select
    Range("Table1[[#Headers],[Date]]").Select
    Selection.End(xlDown).Select
    ActiveCell.Offset(0, 6).Range("A1").Select
 .Hyperlinks.Add Anchor:=Selection, _
 Address:=Filename, _
 ScreenTip:="pdf file report", _
 TextToDisplay:="PDF_click_here"
End With
End Sub

Sub CreateFolder()

If Dir("C:\Users\ManiKandan\Desktop\New folder\clinic\reports", vbDirectory) = "" Then
          MkDir "C:\Users\ManiKandan\Desktop\New folder\clinic"
          retVal = Shell("explorer.exe C:\Users\ManiKandan\Desktop\New folder\clinic\reports", vbNormalFocus)
     Else
          'retVal = Shell("explorer.exe C:\Users\ManiKandan\Desktop\New folder\clinic\reports", vbNormalFocus)
End If
End Sub

