Sub PRINT_COMMAN()
ActiveWorkbook.ActiveSheet.PrintOut FROM:=1, TO:=1, COPIES:=1, COLLATE:=False, PREVIEW:=False
End Sub


Sub Filter_regno()
Range("E7").AutoFilter field:=3, Criteria1:=Range("i2")
'Range("E7").AutoFilter field:=6, Criteria1:=Range("d4").Value
'Range("E7").AutoFilter field:=3, Criteria1:=Range("E3").Value
End Sub

Sub Filter_testname()
Range("e7").AutoFilter field:=3, Criteria1:=Range("E4").Value
End Sub

Sub PasteasValue()

Selection.SpecialCells(xlCellTypeVisible).Select

Selection.PasteSpecial Paste:=xlPasteValues

End Sub

Private Sub Workbook_BeforeClose(Cancel As Boolean)
Application.DisplayAlerts = False
ThisWorkbook.Close SaveChanges:=True
Application.DisplayAlerts = True
End Sub

Sub regup()
'
' regup Macro
'

'
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    ActiveSheet.Unprotect
    Sheets("PATIENT DATA").Select
    Range("Table1[[#Headers],[Reg.No]]").Select
    Selection.End(xlDown).Select
    Selection.Copy
    Sheets("NAME").Select
    Range("E4").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    With Selection.Font
        .ThemeColor = xlThemeColorAccent2
        .TintAndShade = 0.799981688894314
    End With
    With Selection.Interior
        .Pattern = xlSolid
        .PatternThemeColor = xlThemeColorAccent1
        .ThemeColor = xlThemeColorAccent2
        .TintAndShade = 0.799981688894314
        .PatternTintAndShade = 0.799981688894314
    End With
    Range("E4").Select
    Selection.ClearFormats
    With Selection.Font
        .ThemeColor = xlThemeColorAccent2
        .TintAndShade = 0.799981688894314
    End With
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorAccent2
        .TintAndShade = 0.799981688894314
        .PatternTintAndShade = 0
        ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    End With
End Sub


Sub protect_NAME()
Sheets("NAME").Select
ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
End Sub

Private Sub CommandButton1_Click()
Dim path As String
path = "C:\Users\ManiKandan\Desktop\New folder\clinic\reports\"

Dim regno As String
regno = Range("i14")

Dim filename1 As String
filename1 = regno & "-" & Range("b12").Text
Application.DisplayAlerts = False
ActiveSheet.Copy

ActiveSheet.Shapes("commandbutton1").Delete
'pdf
ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, ignoreprintareas:=False, Filename:=path & filename1
'xlsx
Application.DisplayAlerts = False

With ActiveWorkbook
   .SaveAs Filename:=path & filename1 & ".xlsx", FileFormat:=51
   .Close
End With
'MsgBox "your next document no " & regno + 1
Range("i13") = regno + 1
ThisWorkbook.SAVE

Application.DisplayAlerts = True
End Sub


Sub BILL_NO()
'
' BILL_NO Macro
'

'
    Range("E3").Select
    ActiveCell.FormulaR1C1 = ""
    Sheets("BILL").Select
    Range("I14").Select
    Selection.Copy
    Sheets("PATIENT DATA").Select
    Range("E3").Select
    ActiveSheet.Paste
    Sheets("BILL").Select
End Sub

Sub BILLDATA()
'
' BILLDATA Macro
'

'
    Sheets("PATIENT DATA").Select
    Range("Table1[[#Headers],[NAME]]").Select
    Selection.End(xlDown).Select
    Range("H192:I192").Select
    Selection.Copy
    Sheets("BILL").Select
    Selection.PasteSpecial Paste:=xlPasteAll, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=True
    Range("B17").Select
End Sub

Sub dateupdate()
'
' dateupdate Macro
'
' Keyboard Shortcut: Ctrl+a
ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    ActiveSheet.Unprotect
    Selection.Copy
    Range("B3").Select
    Application.CutCopyMode = False
    ActiveCell.FormulaR1C1 = ""
    Range("E3").Select
    Selection.Copy
    Range("B3").Select
    Selection.PasteSpecial Paste:=xlPasteValuesAndNumberFormats, Operation:= _
        xlNone, SkipBlanks:=False, Transpose:=False
    Application.CutCopyMode = False
    Selection.NumberFormat = "d mmmm yyyy-h:mm AM/PM"
    Range("B3").Select
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
End Sub


Sub regnoupdate()

Application.DisplayAlerts = False
  Application.ScreenUpdating = False


' regnoupdate Macro
'call allowmacrowhenprotected()

'Dim lo As ListObject
  
  'Set reference to the first Table on the sheet
  Set lo = Sheet2.ListObjects(1)
  
  'Clear All Filters for entire Table
  Call allowmacrowhenprotected
  lo.AutoFilter.ShowAllData
    Sheets("PATIENT DATA").Select
    Range("G6").Select
    Selection.End(xlDown).Select
    Selection.Copy
    Sheets("NAME").Select
       Range("E4").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Range("F4").Select
    Application.CutCopyMode = False
    ActiveCell.FormulaR1C1 = "=RC[-1]+1"
    Range("F4").Select
    Selection.Copy
    Range("B4").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Application.CutCopyMode = False
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    
End Sub

Sub allowmacrowhenprotected()
'ActiveSheet.Unprotect DrawingObjects:=True, Contents:=True, Scenarios:=True
    ActiveSheet.Unprotect

End Sub
Sub cbc()

' cbc Macro
'
' Keyboard Shortcut: Ctrl+i
'
  Application.DisplayAlerts = False
  Application.ScreenUpdating = False
  Call sampleid
  
  DoEvents
  Application.StatusBar = "updating" & (I)
  
 Set lo = Sheet2.ListObjects("Table1")
    lo.AutoFilter.ShowAllData
    Sheets("PATIENT DATA").Select
    ActiveSheet.Unprotect
    Range("F6").Select
    Selection.End(xlDown).Select
    ActiveCell.Offset(1, 0).Range("A1").Select
    Sheets("NAME").Select
    Range("B3").Select
    Selection.Copy
    Sheets("PATIENT DATA").Select
    ActiveSheet.Paste
    ActiveCell.Offset(0, 1).Range("A1").Select
    Sheets("CBC").Select
    Range("I14").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("PATIENT DATA").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    ActiveCell.Offset(0, 1).Range("A1").Select
    Sheets("CBC").Select
    Range("B12:D12").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("PATIENT DATA").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    ActiveCell.Offset(0, 1).Range("A1").Select
    Sheets("CBC").Select
    Range("B13:D13").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("PATIENT DATA").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    ActiveCell.Offset(0, 1).Range("A1").Select
    Sheets("CBC").Select
    Range("A19").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("PATIENT DATA").Select
    ActiveSheet.Paste
    Selection.Font.Bold = False
    With Selection.Font
        .Name = "Times New Roman"
        .Size = 12
        .Strikethrough = False
        .Superscript = False
        .Subscript = False
        .OutlineFont = False
        .Shadow = False
        .Underline = xlUnderlineStyleNone
        .Color = -16777216
        .TintAndShade = 0
        .ThemeFont = xlThemeFontNone
    End With
    With Selection.Font
        .Name = "Calibri"
        .Size = 12
        .Strikethrough = False
        .Superscript = False
        .Subscript = False
        .OutlineFont = False
        .Shadow = False
        .Underline = xlUnderlineStyleNone
        .Color = -16777216
        .TintAndShade = 0
        .ThemeFont = xlThemeFontMinor
    End With
    ActiveCell.Offset(0, 4).Range("A1").Select
    Sheets("CBC").Select
    Range("E21:E34").Select
    Selection.Copy
    Sheets("PATIENT DATA").Select
    Selection.PasteSpecial Paste:=xlPasteAll, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=True
    ActiveCell.Offset(0, 14).Range("A1").Select
    
   Call sampleidupdate
  
    Sheets("CBC").Select
    Application.CutCopyMode = False
    ActiveCell.Offset(13, 0).Range("A1").Select
    
     Call HYPERLINK
     Sheets("CBC").Select
    Dim path As String
path = "C:\Users\ManiKandan\Desktop\New folder\clinic\reports\"
Dim regno As String
regno = Range("i14")

Dim filename1 As String
filename1 = regno & "-" & Range("b12").Text & "-" & Range("A19")
