Sub cbcprint()
'
' cbcprint Macro
'
' Keyboard Shortcut: Ctrl+g
'
    Sheets("PATIENT DATA").Select
    Range("H13").Select
    Selection.Copy
    Sheets("CBC (2)").Select
    Range("B12:D12").Select
    Application.CutCopyMode = False
    ActiveSheet.Unprotect
    Sheets("PATIENT DATA").Select
    Selection.Copy
    Sheets("CBC (2)").Select
    ActiveSheet.Paste
    Sheets("CBC (2)").Select
    ActiveCell.Offset(1, 0).Range("A1:C1").Select
    Sheets("PATIENT DATA").Select
    Range("I13").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("CBC (2)").Select
    ActiveSheet.Paste
    ActiveCell.Offset(1, 7).Range("A1").Select
    Sheets("PATIENT DATA").Select
    Range("G13").Select
    Application.CutCopyMode = False
    Selection.Copy
    Sheets("CBC (2)").Select
    ActiveSheet.Paste
    Application.CutCopyMode = False
    With Selection
        .HorizontalAlignment = xlLeft
        .VerticalAlignment = xlCenter
        .WrapText = False
        .Orientation = 0
        .AddIndent = False
        .IndentLevel = 0
        .ShrinkToFit = False
        .ReadingOrder = xlContext
        .MergeCells = False
    End With
    ActiveCell.Offset(7, -4).Range("A1").Select
    Sheets("PATIENT DATA").Select
    Range("N13:AA13").Select
    Selection.Copy
    Sheets("CBC (2)").Select
    Selection.PasteSpecial Paste:=xlPasteAll, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=True
    ActiveWindow.ScrollRow = 9
    ActiveWindow.ScrollRow = 10
    ActiveWindow.ScrollRow = 13
    ActiveWindow.ScrollRow = 14
    ActiveWindow.ScrollRow = 15
    ActiveWindow.ScrollRow = 16
    ActiveWindow.ScrollRow = 18
    ActiveWindow.ScrollRow = 21
    ActiveWindow.ScrollRow = 22
    Range("E21:E34").Select
    Range("E34").Activate
    Range("E21:E34").Select
    Range("E34").Activate
    Application.CutCopyMode = False
    Selection.ClearFormats
    With Selection
        .HorizontalAlignment = xlLeft
        .VerticalAlignment = xlBottom
        .WrapText = False
        .Orientation = 0
        .AddIndent = False
        .IndentLevel = 0
        .ShrinkToFit = False
        .ReadingOrder = xlContext
        .MergeCells = False
    End With
    Range("G35").Select
End Sub




