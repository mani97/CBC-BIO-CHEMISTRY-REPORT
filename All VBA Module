Sub sampleidupdate()

Sheets("PATIENT DATA").Select
Range("P3").Select
Selection.Copy
Range("Table1[[#Headers],[Reg.No]]").Select
Selection.End(xlDown).Select
ActiveCell.Offset(0, 4).Range("A1").Select
Selection.PasteSpecial Paste:=xlPasteValues
''Sheets("cbc").Select

End Sub

Sub sampleid()
Sheets("PATIENT DATA").Select
    Range("Table1[[#Headers],[Reg.No]]").Select
    Selection.End(xlDown).Select
    ActiveCell.Offset(0, 4).Range("A1").Select
    Selection.Copy
    Range("Q3").Select
    Selection.PasteSpecial Paste:=xlPasteValues
    
End Sub


    
