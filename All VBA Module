Dim regno As String
regno = Range("i14")

Dim filename1 As String
filename1 = regno & "-" & Range("b12").Text & "-" & Range("C19")
Application.DisplayAlerts = False
ActiveSheet.Copy

'ActiveSheet.Shapes("commandbutton1").Delete
'pdf
'ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, ignoreprintareas:=False, Filename:=path & filename1
'xlsx
Application.DisplayAlerts = False

With ActiveWorkbook
   '.SaveAs Filename:=path & filename1 & ".xlsx", FileFormat:=51
   ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, ignoreprintareas:=False, Filename:=path & filename1

   .Close
End With
'MsgBox "your next document no " & regno + 1
'Range("i13") = regno + 1
ThisWorkbook.SAVE
    
    ActiveWorkbook.ActiveSheet.PrintPreview
End Sub


Sub lftrftlipid()

 Dim path As String
path = "C:\Users\ManiKandan\Desktop\New folder\clinic\reports\"

Dim regno As String
regno = Range("i14")

Dim filename1 As String
filename1 = regno & "-" & Range("b12").Text & "-" & Range("E18")
Application.DisplayAlerts = False
ActiveSheet.Copy

'ActiveSheet.Shapes("commandbutton2").Delete
'pdf
'ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, ignoreprintareas:=False, Filename:=path & filename1
'xlsx
Application.DisplayAlerts = False

With ActiveWorkbook
   '.SaveAs Filename:=path & filename1 & ".xlsx", FileFormat:=51
   ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, ignoreprintareas:=False, Filename:=path & filename1

   .Close
End With
'MsgBox "your next document no " & regno + 1
'Range("i13") = regno + 1
ThisWorkbook.SAVE

